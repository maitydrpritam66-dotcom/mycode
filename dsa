#include<stdio.h>                                                  ///// linear search 
int main(){

    int n , key, b = 0  ;
    printf("enter the size of array : ") ;
    scanf("%d",&n) ;
    int arr[n];
    printf("enter the elements of array of size of %d : ",n) ;
    for (int i = 0; i < n; i++){
        scanf("%d",&arr[i]);
    }
    printf("enter the key element : ") ;
    scanf("%d",&key); // input complete 


    //linear search 
    for (int i = 0; i < n; i++){
        if (arr[i]==key){
            printf("%d item found at %d index \n",key ,i) ; 
            b+=1 ;
        }  
    }
    if (b==0){
        printf("%d didnt found!! \n", key) ;
    }
    
        
 return 0 ;
}




#include<stdio.h>                                       ///// binaary search

void binary_search(int arr[] , int size , int key ){
    int start = 0  , end = size - 1 , b = 0;
    int mid = (start + end)/ 2 ;
    while ( start <= end){
        if (key == arr[mid]){
            printf("key found at index %d \n",mid) ; 
            b++ ;
        }
        if(key < arr[mid]){
            end = mid - 1 ;
        }
        else{
            start = mid + 1 ;
        }
        mid = (start + end)/ 2 ;
    }
    if (b==0){
        printf("key %d didnt found at any index \n",key) ; 
    }
}

int main(){

    int n , key  ;
    printf("enter the size of array : ") ;
    scanf("%d",&n) ;
    int arr[n];
    printf("enter the elements of array of size of %d (in sorted order) : ",n) ;
    for (int i = 0; i < n; i++){
        scanf("%d",&arr[i]);
    }
    printf("enter the key element : ") ;
    scanf("%d",&key); // input complete 


    //binary search 
    binary_search(arr,n , key);


 return 0 ;
}


#include<stdio.h>                                             //selection sort 

void selection_sort(int arr[] , int n ){
    int mini ;
    for (int i = 0; i <= n-1 ; i++){
        mini = i ;
        for (int j = i; j <= n-1; j++){
            if(arr[j]<arr[mini]){
                mini = j ;
            }
        }   
            int temp = arr[mini];
            arr[mini] = arr[i] ;
            arr[i] = temp ;
    }
}


int main(){

    int n  ;
    printf("enter the size of array : ") ;
    scanf("%d",&n) ;
    int arr[n];
    printf("enter the elements of array of size of %d : ",n) ;
    for (int i = 0; i < n; i++){
        scanf("%d",&arr[i]);
    } // input complete 

    selection_sort(arr, n) ;

    printf("sorted array is : ") ;
    for (int i = 0; i < n; i++){
        printf("%d ",arr[i]);
    }//sorted array
    
 return 0 ;
}


#include<stdio.h>                                              /////  insertion sort 

void insertion_sort(int arr[] , int n ){
    for (int i = 0; i <= n-1 ; i++){
        int j = i ;
        while (j>0 && arr[j]<arr[j-1]){
            int temp = arr[j];
            arr[j] = arr[j-1] ;
            arr[j-1] = temp ;
            j--;
        }
    }
}


int main(){

    int n  ;
    printf("enter the size of array : ") ;
    scanf("%d",&n) ;
    int arr[n];
    printf("enter the elements of array of size of %d : ",n) ;
    for (int i = 0; i < n; i++){
        scanf("%d",&arr[i]);
    } // input complete 


    insertion_sort(arr, n) ;


    printf("sorted array is : ") ;
    for (int i = 0; i < n; i++){
        printf("%d ",arr[i]);
    }//sorted array
    
        
 return 0 ;
}





#include<stdio.h>                                    ///// bouble sort

void binary_sort(int arr[] , int n ){
    for (int i = 0; i <= n-1 ; i++){
        for (int j = 0; j <= n-i-1; j++){
            if (arr[j]>arr[j+1]){
                int temp = arr[j];
                arr[j] = arr[j+1] ;
                arr[j+1] = temp ;
            }
        }   
    }
}


int main(){

    int n  ;
    printf("enter the size of array : ") ;
    scanf("%d",&n) ;
    int arr[n];
    printf("enter the elements of array of size of %d : ",n) ;
    for (int i = 0; i < n; i++){
        scanf("%d",&arr[i]);
    } // input complete 


    binary_sort(arr, n) ;


    printf("sorted array is : ") ;
    for (int i = 0; i < n; i++){
        printf("%d ",arr[i]);
    }//sorted array
    
        
 return 0 ;
}






#include<stdio.h>                                    ////// merge sort 

merge(int arr , int l , int mid , int r){
    int i = l ;
    int j = mid+1 ; 
    int k = l ;
    int b[r] ;
    while (i<=mid && j<= r){
        if(arr[i]<=arr[j]){
            b[k]= arr[i] ;
            i++ ; 
        }
        else{
            b[k]= arr[j] ;
            j++ ;
        }
        k++ ; 
   
    if(i>mid){
        while(j<=r){
            b[k] = arr[j] ;
            j++ ; 
            k++ ;
        }
    }
    else{
    if(j>r){
        while(i<=mid){
            b[k] = arr[i] ;
            i++ ; 
            k++ ;
        }
    }    }
}
for (int p = l; p <= r; p++){
    arr[p] = b[p] ;
}

 }

merge_sort(int arr[] , int l , int  r ){
    if(l<r){
        int mid =(l+r)/2 ; 
        merge_sort(arr , l , mid ) ;
        merge_sort(arr , mid+1 , r ) ;
        merge(arr , l , mid , r);
    }
}

int main(){

    int n  ;
    printf("enter the size of array : ") ;
    scanf("%d",&n) ;
    int arr[n];
    printf("enter the elements of array of size of %d : ",n) ;
    for (int i = 0; i < n; i++){
        scanf("%d",&arr[i]);
    } // input complete 


    merge_sort(arr, 0 ,n-1) ;

    printf("sorted array is : ") ;
    for (int i = 0; i < n; i++){
        printf("%d ",arr[i]);
    }//sorted array
    return 0 ;
} 




#include<stdio.h>                                       //// STACK

#define size 5
int stack[size];
int top = -1 ;

void push(int item){
    if (top == size-1){
        printf("stack overflow\n");
    }
    else{
        top++;
        stack[top] = item ;
    }
}

void pop(){
    if (top==-1){
        printf("stack underflow\n");
    }
    else{
        int item= stack[top--];
        printf("%d item poped\n",item);
    }   
}

void ttop(){
    if (top==-1){
        printf("stack underflow\n");
    }
    else{
        printf("%d on top\n",stack[top]);
    }   
}

void display(){
    if (top==-1){
        printf("stack underflow\n");
    }
    else{
        printf("\nstack elements are : ");
        for (int i = 0; i <= top; i++){
            printf("%d ",stack[i]) ;
        }
         printf("\n");
    }   
}


int main(){
int choie , item ;
printf("choices is \n1. push \n2. pop \n3. top \n4. display \n5. exit \n");
while (1){
    printf("enter a choice : ");
    scanf("%d",&choie) ;
    switch (choie){
    case 1:
        printf("enter a number for push : ") ;
        scanf("%d",&item);
        push(item);
        break;
    case 2:
        pop();
        break;
    case 3:
        ttop();
        break;
    case 4:
        display();
        break;
    case 5:
        return 0;
        break;  
    default:
        printf("\nivlaid choice\n");
        break;
    }
}
 return 0;
}




#include<stdio.h>                                        ///// linear queue 

#define size 5
int queue[size] ;
int front = -1 , back = -1 ; 

int main(){
    printf("choices is : \n 1. push \n 2. pop \n 3. display \n 4. exit \n");
    int choice , item ;
    while (1){
        printf("enter a choice :");
        scanf("%d",&choice) ;
        switch (choice){
        case 1:
        if(back == size-1){
            printf("queue is full \n");
        }
        else{
            printf("enter item for push : ") ;
            scanf("%d",&item) ;
            if(front == -1){
                front = back = 0 ; 
                queue[back] = item ;

            }
            else{
                back++ ;
                queue[back] = item ;
            }
        } 
        break;


        case 2:
        if(front == -1){
            printf("queue is empty \n");
        }
        else{
            if(front == back ){
                front = back =  -1 ;
            }
            else{
                front++ ; 
            }
                        } 
        break;

        
        case 3:
        if(front == -1){
            printf("queue is empty \n");
        }
        else{
            for(int i = front ; i<=back ;i++ ){
                printf("%d ",queue[i]);  
        } 
            printf("\n");  
            }

        
        break;

        case 4:
        return 0;
        break; 
        
        default:
        printf("invalaid choice !! \n") ;
            break;
        }
    }
    return 0 ;
}       



#include<stdio.h>                                         ///// circule queue 

#define size 5
int queue[size] ;
int front = -1 , back = -1 ; 

int main(){           // circuler queue
    printf("choices is : \n 1. push \n 2. pop \n 3. display \n 4. exit \n");
    int choice , item ;
    while (1){
        printf("enter a choice :");
        scanf("%d",&choice) ;
        switch (choice){
        case 1:
        if ((front == 0 && back == size-1) || (front == (back+1) % size)) {
            printf("queue overflow \n");
        }

        else{
            printf("enter item for push : ") ;
            scanf("%d",&item) ;
            if(front == -1){
                front = back = 0 ; 
                queue[back] = item ;

            }
            else{
                back = (back + 1) % size;
                queue[back] = item ;
            }
        } 
        break;


        case 2:
        if(front == -1){
            printf("queue is empty \n");
        }
        else{
            if(front == back ){
                front = back =  -1 ;
            }
            else{
                front = (front + 1) % size;

            }
                        } 
        break;

        
        case 3:
        if(front == -1){
            printf("queue is empty \n");
        }
        else{
            printf("queue elements are : ");
            int i = front;
    while (1) {
        printf("%d ", queue[i]);
        if (i == back) break;
        i = (i + 1) % size;
    }    
            printf("\n");  
            }

        
        break;

        case 4:
        return 0;
        break; 
        
        default:
        printf("invalaid choice !! \n") ;
            break;
        }
    }
    return 0 ;
} 



#include <stdio.h>                                 ////// linked list 
#include <stdlib.h>

// Node structure
struct node {
    int data;
    struct node *next;
};

struct node *head = NULL, *temp, *newnode;

/* Insert at beginning */
void begininsert() {
    newnode = (struct node *)malloc(sizeof(struct node));
    if (newnode == NULL) {
        printf("Overflow\n");
    } else {
        printf("Enter value: ");
        scanf("%d", &newnode->data);
        newnode->next = head;
        head = newnode;
        printf("Item inserted at beginning\n");
    }
}

/* Insert at end */
void lastinsert() {
    newnode = (struct node *)malloc(sizeof(struct node));
    if (newnode == NULL) {
        printf("Overflow\n");
    } else {
        printf("Enter value: ");
        scanf("%d", &newnode->data);
        newnode->next = NULL;3

        if (head == NULL) {
            head = newnode;
        } else {
            temp = head;
            while (temp!= NULL) {
                temp = temp->next;
            }
            temp->next = newnode;
        }
        printf("Item inserted at end\n");
    }
}

/* Delete from beginning */
void begin_delete() {
    if (head == NULL) {
        printf("List is empty\n");
    } else {
        temp = head;
        head = head->next;
        printf("%d deleted from beginning\n", temp->data);
        free(temp);
    }
}

/* Delete from end */
void last_delete() {
    struct node *prev;
    if (head == NULL) {
        printf("List is empty\n");
    } 
    else if (head->next == NULL) {
        printf("%d deleted\n", head->data);
        free(head);
        head = NULL;
    } 
    else {
        temp = head;
        while (temp->next != NULL) {
            prev = temp;
            temp = temp->next;
        }
        prev->next = NULL;
        printf("%d deleted from end\n", temp->data);
        free(temp);
    }
}

/* Display list */
void display() {
    if (head == NULL) {
        printf("List is empty\n");
    } else {
        temp = head;
        printf("Linked List: ");
        while (temp != NULL) {
            printf("%d ", temp->data);
            temp = temp->next;
        }
        printf("\n");
    }
}

/* Main function */
int main() {
    int choice;
        printf("choices is : \n 1. Insert at begining \n 2. Insert at end \n 3. Delete from beginning \n 4. Delete from end \n 5. Display \n 6. Exit \n");
    while (1) {
        printf("\nEnter choice: ");
        scanf("%d", &choice);

        switch (choice) {
        case 1:
            begininsert();
            break;
        case 2:
            lastinsert();
            break;
        case 3:
            begin_delete();
            break;
        case 4:
            last_delete();
            break;
        case 5:
            display();
            break;
        case 6:
            return 0;
        default:
            printf("Invalid choice\n");
        }
    }
}
